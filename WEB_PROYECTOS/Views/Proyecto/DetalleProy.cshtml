@model ENTIDAD.Proyecto

@{
    ViewBag.Title = "Detalles del Proyecto";
}

<div>
    <h4>Detalles del Proyecto</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            <label> Nombre del Proyecto:</label>
        </dt>

        <dd>
            @Model.NombreProyecto
        </dd>

        <dt>
            <label>Fecha de Inicio:</label>
        </dt>

        <dd>
            @Model.FechaInicio.ToShortDateString()
        </dd>

        <dt>
            <label>Fecha Fin:</label>
        </dt>

        <dd>
            @Model.FechaFin.ToShortDateString()
        </dd>

    </dl>
</div>
<p>
    <a class="btn btn-primary"  href="@Url.Action("Modificar", "Proyecto", new { id = Model.ProyectoId})">Editar</a>
    <a class="btn btn-default" href="@Url.Action("Index", "Proyecto")">Regresar</a>
</p>
